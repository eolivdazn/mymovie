{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mymovie/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mymovie/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mymovie",
  "projects": {
    "mymovie": {
      "type": "application",
      "root": "apps/mymovie",
      "entryFile": "main",
      "sourceRoot": "apps/mymovie/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mymovie/tsconfig.app.json"
      }
    },
    "movies": {
      "type": "application",
      "root": "apps/movies",
      "entryFile": "main",
      "sourceRoot": "apps/movies/src",
      "compilerOptions": {
        "tsConfigPath": "apps/movies/tsconfig.app.json"
      }
    }
  }
}